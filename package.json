{
  "name": "@ng-vcl/ng-vcl",
  "version": "0.6.0-2",
  "description": "A comprehensive library of components for Angular 6+ with VCL based styling",
  "scripts": {
    "lint": "tslint --type-check -p ./tsconfig.json",
    "lint:src": "tslint --type-check -p ./src/tsconfig.json",
    "dev": "cross-env MODE=development webpack-serve --config webpack.config.js",
    "docs:clean": "rimraf docs",
    "docs:pull-current": "npm run docs:clean && git clone -b gh-pages --single-branch https://github.com/ng-vcl/ng-vcl.git docs",
    "docs:build": "rimraf docs/$TRAVIS_BRANCH && cross-env MODE=production GIT_BRANCH=$TRAVIS_BRANCH webpack",
    "docs:deploy": "git-directory-deploy --directory docs --branch gh-pages",
    "docs": "npm-run-all lint  docs:pull-current docs:build",
    "build:clean": "rimraf dist",
    "build:gulp": "gulp build",
    "build": "npm-run-all lint:src build:clean build:gulp",
    "publish": "gulp publish",
    "publish:next": "gulp publish:next",
    "publish:git:ng-vcl": "git-directory-deploy --directory dist/@ng-vcl/ng-vcl --branch dist_v0.6",
    "publish:git:ng-vcl:jss": "git-directory-deploy --directory dist/@ng-vcl/jss-form --branch dist_v0.6_jss",
    "clean": "npm-run-all build:clean docs:clean",
    "docker": "docker run -it -v $(pwd):/app -p 0.0.0.0:3000:3000 node:6.9.1 /bin/bash /app/entrypoint.bash",
    "test": "echo \"Error: no test specified\" && exit 1",
    "markdownlint": "markdownlint -c markdownlint.json src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-vcl/ng-vcl.git"
  },
  "keywords": [
    "web-component",
    "angular",
    "VCL"
  ],
  "author": "The ng-vcl authors",
  "license": "MIT",
